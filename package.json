{
  "name": "stapp",
  "version": "0.0.1",
  "description": "Stapp Frontend boilerplate",
  "homepage": "https://github.com/theaidem/stapp",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run lint && npm run clean && npm run build:webpack",
    "build:webpack": "NODE_ENV=production node -r babel-register ./node_modules/.bin/webpack --config webpack.config.prod.js",
    "deploy:gh-pages": "GH_PAGES=true npm run build && ./node_modules/.bin/gh-pages -d dist && npm run clean",
    "start": "clear && node -r babel-register ./node_modules/.bin/webpack-dev-server --config webpack.config.dev.js",
    "gen": "clear && node ./scripts/generator.js",
    "lint": "./node_modules/.bin/eslint --ignore-path .gitignore --ext .js --ext .jsx source/scripts",
    "lint:fix": "clear && ./node_modules/.bin/eslint --fix --ext .js --ext .jsx source/scripts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint-staged": "lint-staged"
  },
  "author": "Max Kokorin",
  "license": "MIT",
  "dependencies": {
    "connected-react-router": "^4.3.0",
    "favicons-webpack-plugin": "^0.0.7",
    "history": "^4.6.2",
    "inquirer": "^3.3.0",
    "pluralize": "^7.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.5",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.24.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "ejs-loader": "^0.3.0",
    "eslint": "^4.7.1",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-react": "^7.3.0",
    "eventsource-polyfill": "^0.9.6",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "gh-pages": "^1.0.0",
    "handlebars-loader": "^1.5.0",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.3.0",
    "pre-commit": "^1.2.2",
    "react-transform-hmr": "^1.0.1",
    "rimraf": "^2.5.1",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-hot-middleware": "^2.6.4"
  },
  "lint-staged": {
    "*.{js,jsx}": "lint"
  },
  "pre-commit": "lint-staged"
}
